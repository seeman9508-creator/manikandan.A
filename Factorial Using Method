import java.io.*;
import java.util.*;

public class Solution {

    void fac(int x) {
        int sum = 1;

        for (int i = 1; i <= x; i++) {
            sum = sum * i;
        }

        System.out.println(sum);
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        Solution ob = new Solution();

        int x = sc.nextInt();
        ob.fac(x);
    }
}
