import java.io.*;
import java.util.*;

public class Solution {

    public int large(int a, int b, int c) {
        if (a >= b && a >= c) {
            return a;
        } 
        else if (b >= a && b >= c) {
            return b;
        } 
        else {
            return c;
        }
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        Solution ob = new Solution();
        
        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();
        
        System.out.println(ob.large(a, b, c));
    }
}
