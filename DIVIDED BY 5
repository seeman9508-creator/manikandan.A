import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        if(n%5==0)
        {
            System.out.println("Yes");
        }
        else
        {
            System.out.println("No");
            
        }
    }
}
