import java.io.*;
import java.util.*;


class Vehicle {
    String brand;

    void showBrand() {
        System.out.println("Brand:" + brand);
    }
}

class Car extends Vehicle {
    int speed;

    void showSpeed() {
        System.out.println("Speed:" + speed);
    }
}

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        Car car = new Car();

        car.brand = sc.nextLine();
        car.speed = sc.nextInt();

        car.showBrand();
        car.showSpeed();

        
}
}
