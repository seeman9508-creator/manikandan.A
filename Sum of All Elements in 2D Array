import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
   Scanner sc = new Scanner(System.in);

        int R = sc.nextInt(); 
        int C = sc.nextInt();   

        int sum = 0;

     
        for (int i = 0; i < R; i++) {
            for (int j = 0; j < C; j++) {
                sum += sc.nextInt();
            }
        }

        System.out.println(sum);
        sc.close();
    }
}
    
