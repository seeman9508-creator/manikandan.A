import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int N=s.nextInt();
        int min=Integer.MAX_VALUE;
        int max=Integer.MIN_VALUE;
        for(int i=0;i<N;i++)
        {
            int num=s.nextInt();
            if(num<min){
                min=num;
            }
            if(num>max){
        max=num;
        }
}
System.out.println(max-min);
}
}
