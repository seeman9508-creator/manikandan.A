import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
              Scanner sc=new Scanner(System.in);
        char ch=sc.next().charAt(0);
        switch(Character.toLowerCase(ch)){
            case 'a':
            case 'e':
            case 'i':
            case 'u':
                System.out.print("Vowel");
                break;
            default:
                System.out.println("Consonant");
        }
        sc.close();
    }
}
